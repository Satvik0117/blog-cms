    <!-- s-content
    ================================================== -->
    <section class="s-content">
        
        <div class="row masonry-wrap">
            <div class="masonry">

                <div class="grid-sizer"></div>
            <% for(var i=0;i<blogs.length;i++){  %>
                <% if(i==0){ %>
                 <article class="masonry__brick entry format-standard" data-aos="fade-up">
                        
                    <div class="entry__thumb">
                        <a href="/blog/<%=blogs[i]._id%>" class="entry__thumb-link">
                            <img src="/uploads/<%=blogs[i].imgPath%>" 
                                    alt="">
                        </a>
                    </div>
    
                    <div class="entry__text">
                        <div class="entry__header">
                            
                            <div class="entry__date">
                                <a href="/blog/<%=blogs[i]._id%>"><%=blogs[i].created.toDateString() %></a>
                            </div>
                            <h1 class="entry__title"><a href="/blog/<%=blogs[i]._id%>"><%=blogs[i].title %></a></h1>
                            
                        </div>
                        <div class="entry__excerpt">
                            <p><%=blogs[i].intro_text %>
                            </p>
                        </div>
                        <div class="entry__meta">
                            <span class="entry__meta-links">
                             
                            </span>
                        </div>
                    </div>
    
                </article> <!-- end article -->
                
                <!-- just a quote -->
                
                
                <article class="masonry__brick entry format-quote" data-aos="fade-up">
                        
                    <div class="entry__thumb">
                        <blockquote>
                                <p>Any pilot can describe the mechanics of flying. What it can do for the spirit of man is beyond description.</p>
                                <cite>Barry Goldwater</cite>
                        </blockquote>
                    </div>   
    
                </article> <!-- end article -->
                
                
                <!-- quote ends -->
            
            
          <%  }else{ %>

            <article class="masonry__brick entry format-standard" data-aos="fade-up">
                        
                <div class="entry__thumb">
                    <a href="/blog/<%=blogs[i]._id%>" class="entry__thumb-link">
                        <img src="/uploads/<%=blogs[i].imgPath%>" 
                                alt="">
                    </a>
                </div>

                <div class="entry__text">
                    <div class="entry__header">
                        
                        <div class="entry__date">
                            <a href="/blog/<%=blogs[i]._id%>"><%=blogs[i].created.toDateString() %></a>
                        </div>
                        <h1 class="entry__title"><a href="/blog/<%=blogs[i]._id%>"><%=blogs[i].title %></a></h1>
                        
                    </div>
                    <div class="entry__excerpt">
                        <p><%=blogs[i].intro_text %>
                        </p>
                    </div>
                    <div class="entry__meta">
                        <span class="entry__meta-links">
                          
                        </span>
                    </div>
                </div>

            </article>  <!-- end article -->
      <%      }  %>
                

      <%     } %>
            </div> <!-- end masonry -->
